<head>
    <style>
        #map {
            height: 400px;
            width: 100%;
        }
    </style>
</head>
<body>
    
    <div id="map"></div>
    <script>
      function initMap() {
          var uluru = { lat: 40.7571841, lng: -73.892313 };
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 13,
          center: uluru
        });
        var marker = new google.maps.Marker({
            position: uluru,
          map: map
        });
      }
    </script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB9UXGkliWuJRyUPqQnEEbPyoQ7PMOoytE&callback=initMap">
    </script>
</body>

